{
    "name": "@tempestive/timespan.js",
    "version": "0.1.4",
    "description": "Stop using magic numbers for time related values",
    "author": "Andrea Minato",
    "license": "MIT",
    "source": "src/main.ts",
    "unpkg": "./dist/@tempestive/timespan.umd.js",
    "types": "./dist/types/main.d.ts",
    "keywords": [],
    "main": "./dist/@tempestive/timespan.umd.js",
    "module": "./dist/@tempestive/timespan.es.js",
    "exports": {
        ".": {
            "import": "./dist/@tempestive/timespan.es.js",
            "require": "./dist/@tempestive/timespan.umd.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "test": "ava",
        "buildts": "vite build && tsc --emitDeclarationOnly"
    },
    "devDependencies": {
        "ava": "^3.15.0",
        "esm": "^3.2.25",
        "typescript": "^4.5.4",
        "vite": "^2.9.2"
    },
    "ava": {
        "require": [
            "esm"
        ]
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "directories": {
        "test": "tests"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tempestive/TimeSpan.git"
    },
    "bugs": {
        "url": "https://github.com/tempestive/TimeSpan/issues"
    },
    "homepage": "https://github.com/tempestive/TimeSpan#readme"
}
